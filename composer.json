{
    "name": "r4nkt/laravel-r4nkt-webhooks",
    "description": "Handle R4nkt webhook calls in a Laravel app",
    "keywords": [
        "r4nkt",
        "gamification",
        "leaderboard",
        "achievements",
        "api",
        "sdk",
        "laravel-r4nkt-webhooks"
    ],
    "homepage": "https://github.com/r4nkt/laravel-r4nkt-webhooks",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Elkins",
            "email": "travis.elkins+github@gmail.com",
            "homepage": "https://r4nkt.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/http": "~5.8.0|^6.0|^7.0",
        "illuminate/routing": "~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "~3.8.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "R4nkt\\LaravelWebhooks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R4nkt\\LaravelWebhooks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "R4nkt\\LaravelWebhooks\\R4nktWebhooksServiceProvider"
            ]
        }
    }
}
